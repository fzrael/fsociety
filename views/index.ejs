    .social-link {
      position: relative;
      display: flex;
      align-items: center;
      gap: 0.8rem;
      padding: 0.8rem 1.5rem; /* تقليل المساحة الداخلية */
      background: rgba(255, 255, 255, 0.05);
      border-radius: 12px;
      color: var(--primary-color);
      text-decoration: none;
      transition: all 0.3s ease;
      border: 1px solid rgba(255, 255, 255, 0.1);
      overflow: hidden;
      min-width: 160px; /* تقليل العرض الأدنى */
      justify-content: center;
    }

    .social-link::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: linear-gradient(45deg, transparent, rgba(0, 230, 118, 0.1), transparent);
      transform: translateX(-100%);
      transition: transform 0.5s ease;
    }

    .social-link:hover::before {
      transform: translateX(100%);
    }

    .social-link i {
      font-size: 1.2rem; /* تصغير حجم الأيقونات */
    }

    .social-hover {
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 230, 118, 0.1);
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 0.5rem;
      opacity: 0;
      transition: all 0.3s ease;
    }

    .social-link:hover .social-hover {
      opacity: 1;
    }

    .footer-bottom {
      border-top: 1px solid rgba(255, 255, 255, 0.1);
      padding: 2rem 0;
      margin-bottom: 0;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 1.5rem;
      width: 100%;
    }

    .footer-nav {
      display: flex;
      gap: 1.5rem; /* تقليل المسافة بين الروابط */
      flex-wrap: wrap;
      justify-content: center;
      width: 100%;
    }

    .footer-nav a {
      color: rgba(255, 255, 255, 0.7);
      text-decoration: none;
      transition: all 0.3s ease;
      white-space: nowrap;
      font-size: 0.9rem; /* تصغير حجم النص */
    }

    .footer-nav a:hover {
      color: var(--primary-color);
    }

    .copyright {
      display: flex;
      align-items: center;
      gap: 2rem;
      color: rgba(255, 255, 255, 0.6);
      flex-wrap: wrap;
      justify-content: center;
      width: 100%;
