  <script>
    function getBreachText(count) {
      if (count === 0) return 'لا توجد تسريبات';
      if (count === 1) return 'تسريب واحد';
      if (count === 2) return 'تسريبين';
      if (count >= 3 && count <= 10) return `${count} تسريبات`;
      return `${count} تسريب`;
    }

    function flattenBreaches(breaches) {
      if (!breaches) return [];
      // إذا كانت المصفوفة متداخلة، نقوم بتسطيحها
      return breaches.flat();
    }

    function formatBreachDetails(breach) {
      return `
        <div class="breach-detail">
          <h3>${breach.breach}</h3>
          <p class="breach-date">تاريخ التسريب: ${breach.xposed_date}</p>
          <p class="breach-records">عدد السجلات المسربة: ${breach.xposed_records.toLocaleString()}</p>
          <p class="breach-description">${breach.details}</p>
          <div class="breach-meta">
            <span class="tag">النطاق: ${breach.domain}</span>
            <span class="tag">القطاع: ${breach.industry}</span>
            <span class="tag">مستوى أمان كلمة المرور: ${breach.password_risk}</span>
          </div>
          <div class="exposed-data">
            <p>البيانات المسربة:</p>
            <div class="data-tags">
              ${breach.xposed_data.split(';').map(data => `<span class="tag">${data}</span>`).join('')}
            </div>
          </div>
        </div>
      `;
    }

    function formatAnalytics(analytics) {
      if (!analytics) return '';
      
      const riskInfo = analytics.BreachMetrics.risk[0];
      // تحديد مستوى الخطورة بناءً على الدرجة
      let riskLevel = 'low';
      if (riskInfo.risk_score > 10) {
        riskLevel = 'high';
      } else if (riskInfo.risk_score > 5) {
        riskLevel = 'medium';
      }

      return `
        <div class="analytics-section">
          <h3>📊 تحليل المخاطر</h3>
          <div class="risk-score">
            <p>مستوى الخطورة: <span class="tag ${riskLevel}">${riskLevel === 'high' ? 'مرتفع' : riskLevel === 'medium' ? 'متوسط' : 'منخفض'}</span></p>
            <p>درجة المخاطرة: ${riskInfo.risk_score}/10</p>
          </div>
          
          <h3>🔒 قوة كلمات المرور المسربة</h3>
          <div class="password-strength">
            ${Object.entries(analytics.BreachMetrics.passwords_strength[0])
              .filter(([_, value]) => value > 0)
              .map(([type, count]) => `<span class="tag">${type}: ${count}</span>`).join('')}
          </div>
        </div>
      `;
    }

    const form = document.getElementById('searchForm');
    form.addEventListener('submit', async (e) => {
      e.preventDefault();
      const button = form.querySelector('button');
      button.classList.add('loading');
      
      const email = form.email.value;
      const licenseKey = form.licenseKey.value;
      
      try {
        // التحقق من وجود الترخيص
        if (!licenseKey) {
          Swal.fire({
            title: 'خطأ!',
            text: 'الرجاء إدخال مفتاح الترخيص للبحث المتقدم',
            icon: 'error',
            confirmButtonText: 'حسناً'
          });
          button.classList.remove('loading');
          return;
        }

        // التحقق من صحة الترخيص
        const licenseResponse = await fetch('/api/verify-license', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({ licenseKey })
        });
        
        const licenseData = await licenseResponse.json();
        
        if (!licenseData.valid) {
          Swal.fire({
            title: 'خطأ!',
            text: licenseData.message,
            icon: 'error',
            confirmButtonText: 'حسناً'
          });
          button.classList.remove('loading');
          return;
        }
        
        // إجراء البحث المتقدم
        const response = await fetch(`/search?email=${encodeURIComponent(email)}&deep=true&licenseKey=${encodeURIComponent(licenseKey)}`);
        const data = await response.json();
        
        if (data.error) {
          Swal.fire({
            title: 'خطأ!',
            text: data.error,
            icon: 'error',
            confirmButtonText: 'حسناً',
            background: '#1a1a1a',
            color: '#fff'
          });
        } else if (data.result && data.result.breaches && data.result.breaches.length > 0) {
          const breaches = flattenBreaches(data.result.breaches);
          let htmlContent = `
            <div class="breach-info">
              <p>تم العثور على بريدك في ${getBreachText(breaches.length)}:</p>
              <div class="breach-list">
                ${breaches.map(breach => `<div class="breach-item">${breach}</div>`).join('')}
              </div>
            </div>
          `;

          if (data.analytics) {
            htmlContent += `
              <div class="analytics-info">
                ${formatAnalytics(data.analytics)}
                <div class="detailed-breaches">
                  <h3>📑 تفاصيل التسريبات</h3>
                  ${data.analytics.ExposedBreaches.breaches_details.map(formatBreachDetails).join('')}
                </div>
              </div>
            `;
          }

          Swal.fire({
            title: '⚠️ تحذير: تم اختراق بريدك الإلكتروني!',
            html: htmlContent,
            icon: 'warning',
            confirmButtonText: 'فهمت',
            background: '#1e1e1e',
            customClass: {
              popup: 'swal-dark'
            },
            footer: '<div class="modal-footer" style="display: flex; flex-direction: column; gap: 1rem;">' +
                    '<a href="/download" class="download-btn" style="background: rgba(0, 230, 118, 0.1); color: var(--primary-color); border: 1px solid var(--primary-color); padding: 0.6rem 1rem; border-radius: 6px; text-decoration: none; text-align: center; font-weight: 500; transition: all 0.3s ease; display: inline-flex; align-items: center; justify-content: center; gap: 0.5rem; width: auto; font-size: 0.9rem; margin: 0 auto;">' +
                    '<i class="fas fa-download" style="font-size: 0.9rem;"></i>تحميل FsocietyData</a>' +
                    '</div>'
          });
        } else {
          Swal.fire({
            title: '✅ رائع! بريدك آمن',
            text: 'لم يتم العثور على بريدك في أي تسريبات معروفة',
            icon: 'success',
            confirmButtonText: 'حسناً'
          });
        }
      } catch (error) {
        Swal.fire({
          title: 'خطأ!',
          text: 'حدث خطأ أثناء البحث',
          icon: 'error',
          confirmButtonText: 'حسناً'
        });
      } finally {
        button.classList.remove('loading');
      }
    });

    document.getElementById('freeSearchLink').addEventListener('click', async (e) => {
      e.preventDefault();
      
      const { value: email } = await Swal.fire({
        icon: 'info',
        title: 'fsocietyFree',
        input: 'email',
        // inputLabel: 'أدخل بريدك الإلكتروني',
        inputPlaceholder: 'أدخل بريدك الإلكتروني',
        confirmButtonText: 'بحث',
        showCloseButton: true,
        background: '#1a1a1a',
        color: '#fff',
        customClass: {
          popup: 'swal2-dark',
          input: 'dark-input'
        },
        inputValidator: (value) => {
          if (!value) {
            return 'الرجاء إدخال بريد إلكتروني';
          }
        }
      });

      if (email) {
        try {
          const response = await fetch(`/free-search?email=${encodeURIComponent(email)}`);
          const data = await response.json();
          
          if (data.error) {
            Swal.fire({
              title: 'خطأ!',
              text: data.error,
              icon: 'error',
              confirmButtonText: 'حسناً',
              background: '#1a1a1a',
              color: '#fff'
            });
          } else {
            if (data.breachCount > 0) {
              Swal.fire({
                title: '⚠️ تحذير',
                html: `تم العثور على بريدك في <span style="color: #00a67c; font-weight: bold;">${data.breachCount}</span> مواقع`,
                icon: 'warning',
                confirmButtonText: 'فهمت',
                footer: '<a href="/pricing" class="swal2-footer-link">يرجى شراء الترخيص للبحث المتقدم</a>',
                background: '#1a1a1a',
                color: '#fff',
                customClass: {
                  popup: 'swal2-dark'
                }
              });
            } else {
              Swal.fire({
                title: '✅ رائع! بريدك آمن',
                text: 'لم يتم العثور على بريدك في أي تسريبات معروفة',
                icon: 'success',
                confirmButtonText: 'حسناً',
                background: '#1a1a1a',
                color: '#fff'
              });
            }
          }
        } catch (error) {
          Swal.fire({
            title: 'خطأ!',
            text: 'حدث خطأ أثناء البحث',
            icon: 'error',
            confirmButtonText: 'حسناً',
            background: '#1a1a1a',
            color: '#fff'
          });
        }
      }
    });

    // إزالة تأثير الضباب بعد انتهاء الانيميشن
    setTimeout(() => {
      document.querySelector('.container').classList.add('clear');
    }, 3000);
  </script>

